<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2014 sp1 (http://www.altova.com) by Donald Phillips (private) -->
<!-- Create initial draft consistent with Core Component Submission Form -->
<xs:schema xmlns="urn:org:pesc:message:AcademicEportfolio:v1.0.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:core="urn:org:pesc:core:CoreMain:v1.16.0" xmlns:AcRec="urn:org:pesc:sector:AcademicRecord:v1.10.0" xmlns:AcademicEportfolio="urn:org:pesc:message:AcademicEportfolio:v1.0.0" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" xmlns:AdmRec="urn:org:pesc:sector:AdmissionsRecord:v1.4.0" targetNamespace="urn:org:pesc:message:AcademicEportfolio:v1.0.0" elementFormDefault="unqualified" attributeFormDefault="unqualified" vc:minVersion="1.1">
	<!--                              -->
	<xs:import namespace="urn:org:pesc:core:CoreMain:v1.16.0" schemaLocation="CoreMain_v1.16.0.xsd"/>
	<xs:import namespace="urn:org:pesc:sector:AcademicRecord:v1.10.0" schemaLocation="AcademicRecord_v1.10.0.xsd"/>
	<xs:import namespace="urn:org:pesc:sector:AdmissionsRecord:v1.4.0" schemaLocation="AdmissionsRecord_v1.4.0.xsd"/>
	<!--Root Element-->
	<xs:element name="AcademicEPortfolio" type="AcademicEPortfolioType">
		<xs:annotation>
			<xs:documentation>Root element of Academic ePortfolio message</xs:documentation>
		</xs:annotation>
	</xs:element>
	<!--                              -->
	<!--=========================================-->
	<!--Academic ePortfolio Complex Types-->
	<!--=========================================-->
	<xs:complexType name="AcademicEPortfolioType">
		<xs:annotation>
			<xs:documentation>A type defining contents of the owner's academic eportfolio.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TransmissionData" type="TransmissionDataType">
				<xs:annotation>
					<xs:documentation>This element provides a logical grouping of elements related to Transmission information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Profile" type="ProfileType">
				<xs:annotation>
					<xs:documentation>Student identification and demographical information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Events" type="EventType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Attributes describing an event as stored in a student’s portfolio. An event could be related to an academic course or program or a life/work/education event or experience.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Goals" type="GoalType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A student’s goals and related action plans as stored in the student’s portfolio. Active and completed goals can be defined.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Artifacts" type="ArtifactType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The Artifacts component provides a flexible structure for storing artifacts related to many of the other components of the schema. For example, artifacts could be related to events, goals, education, employment, or competencies. Artifacts can represent documents uploaded into the eportfolio or online documents stored at a specific URL.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Comments" type="CommentType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The Comments component supports the related Events and Artifacts components providing a structure for owner, advisor, teacher, professor, peer, and parent/guardian eportfolio comments that can be connected to other related objects. The same comment can refer to multiple objects in the instance.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Education" type="EducationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains score and interpretive data for all the tests reported in the test score report</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EmploymentHistory" type="EmploymentHistoryType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The Employment History component supports data related to jobs at multiple employers and a place for ObjectLinks to artifacts such as résumés stored in the student’s portfolio. This employment history data could be used by a system as part of a résumé function.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Competencies" type="CompetencyType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The Competencies component provides a general structure for data related to the eportfolio owner’s competencies and skills. A general structure was defined to support the numerous skill and competency models and taxonomies in existence.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Networks" type="NetworksType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The Networks component includes elements to describe the eportfolio owner’s networks and references.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LicensesAndCredentials" type="LicensesCertificationsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The Licenses and Credentials component defines the eportfolio owner’s licenses—including information on the currency of the license and the issuing authority—as well as industry-recognized credentials.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MilitaryHistory" type="MilitaryServiceType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The Military History component provides a summary of the eportfolio owner’s military employment experiences.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CareerAssessments" type="CareerAssessmentType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The Career Assessments component provides a structure for supporting a range of career assessment results including two standard Department of Labor assessments commonly used in education and workforce development settings.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Lists" type="ListsType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The Lists component defines a flexible structure for transmitting or exporting lists of stored items that are typically part of career and education planning in a student’s portfolio. This includes lists of careers of interest, postsecondary programs and majors of interest, postsecondary schools of interest, and scholarships.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UserDefinedExtensions" type="core:UserDefinedExtensionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is used to create a place holder for the Unique Data which is defined by the VendorCode and IdentifierCode.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NoteMessage" type="core:NoteMessageType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation> Note Message: Entities may use this field to store information not otherwise specified.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ProfileType">
		<xs:annotation>
			<xs:documentation>A complex type describing profile information for the portfolio owner.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="core:NameType">
				<xs:annotation>
					<xs:documentation>The title given to someone, usually at birth. The name of the author of the comment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Gender" type="core:GenderType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The sex designation of a person.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Birth" type="core:BirthType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information about the delivery, creation or beginning of life of a person.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SSN" type="core:SSNType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The person's U.S. Social Security Number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SIN" type="core:SINIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The Social Insurance Number (Canadian) of the portfolio owner.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NSN" type="core:NSNIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The National Student Number (Canadian) of the portfolio owner.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgencyAssignedID" type="core:AgencyAssignedIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The ID assigned by an agency to the person (student or borrower).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Contacts" type="AcRec:ContactsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This element provides a logical grouping of elements related to contact information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Citizenship" type="core:CitizenshipType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Attributes of a person's qualifications to become or to have become a citizen of a country.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EthnicityRace" type="core:EthnicityRaceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The cultural, tribal or national background of a person.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ParentGuardian" type="core:ParentGuardianType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>To be deprecated in a future release</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EducationType">
		<xs:annotation>
			<xs:documentation>A complex type describing the portfolio owner’s education.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="HSTranscript" type="HSTranscriptType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A portfolio owner's high school transcript.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CollegeTranscript" type="CollegeTranscriptType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A portfolio owner's College transcript.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TestScoreReport" type="AcRec:TestScoreReportType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Information about the attribuites, accomplishments and test scores of a set of students</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ApplicationForAdmission" type="ApplicationForAdmissionType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A portfolio owner’s application to a postsecondary institution.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PostsecondaryProgram" type="PostsecondaryProgramType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A portfolio owner's postsecondary program; both completed and in-progress.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SecondaryEducation" type="SecondaryEducationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A portfolio owner's high school education details.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OtherLearning" type="OtherLearningType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A portfolio owner's other learning experiences.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GoalType">
		<xs:annotation>
			<xs:documentation>A complex type describing a goal defined by the portfolio owner.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="GoalID" type="ObjectIDSimpleType">
				<xs:annotation>
					<xs:documentation>An identifier for a goal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GoalCode" type="GoalCodeSimpleType">
				<xs:annotation>
					<xs:documentation>The type of goal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GoalTitle" type="xs:string">
				<xs:annotation>
					<xs:documentation>The title or label for the goal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GoalDescription" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A brief description of the goal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GoalTargetDate" type="SeparatedDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The target date for achieving the goal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GoalCompletionDate" type="SeparatedDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The actual date when the goal was achieved.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GoalObjectLink" type="ObjectLinkType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>An object link to another node or object related to this goal. For example, it could connect this goal to a related artifact stored in the portfolio.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GoalActionPlanStep" type="ActionPlanStepType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Details describing the attributes of a single step in an action plan related to a goal set by the portfolio owner.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ObjectLinkType">
		<xs:annotation>
			<xs:documentation>A complex type describing a link between related items stored in a portfolio.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ObjectPath" type="xs:string">
				<xs:annotation>
					<xs:documentation>Path expression specifying a node within this XML document. The node specified by the expression has a parent or child relationship to the current node/object.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ObjectRelationCode" type="ObjectRelationSimpleType">
				<xs:annotation>
					<xs:documentation>The relationship between the linked node/object and the current node/object.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ObjectID" type="ObjectIDSimpleType">
				<xs:annotation>
					<xs:documentation>An alternate identifier, unique within this component, to the node/object within this XML document that has a relationship to the current node. Can be used by applications to reference the related node. Examples of ID elements in the schema: EventID, ArtifactID, CommentID, GoalID, and StepID.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EventType">
		<xs:annotation>
			<xs:documentation>A complex type describing an event documented in the portfolio.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EventID" type="ObjectIDSimpleType">
				<xs:annotation>
					<xs:documentation>An identifier for an event.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EventClassification" type="EventClassificationType">
				<xs:annotation>
					<xs:documentation>The classification of an event.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EventTitle" type="xs:string">
				<xs:annotation>
					<xs:documentation>The name, title, or label of the event.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EventStartDate" type="SeparatedDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The start date of the event.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EventEndDate" type="SeparatedDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The end date of the event.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EventDescription" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A brief description of the event.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrganizationName" type="core:OrganizationNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of Non-person entity such as Organization, Institution, Agency or Business.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EventOrganizationDescription" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A brief description of an organization related to an event.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EventObjectLink" type="ObjectLinkType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>An object link to another node or object related to this event. For example, it could connect this event to a goal defined in the portfolio.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CommentType">
		<xs:annotation>
			<xs:documentation>A complex type describing a comment stored in a portfolio.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CommentID" type="ObjectIDSimpleType">
				<xs:annotation>
					<xs:documentation>An identifier for a comment. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommentDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Date comment was created or saved in the portfolio. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommentTitle" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Title of the comment. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommentDescription" type="xs:string">
				<xs:annotation>
					<xs:documentation>Comment text or description. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommentAuthor" type="AuthorType">
				<xs:annotation>
					<xs:documentation>Comment author name and details. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommentObjectLink" type="ObjectLinkType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Object link to another node or object related to this comment. For example, it could connect this comment to a related artifact stored in the portfolio. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ArtifactType">
		<xs:annotation>
			<xs:documentation>A complex type describing an artifact stored in a portfolio.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ArtifactID" type="ObjectIDSimpleType">
				<xs:annotation>
					<xs:documentation>An identifying code for an artifact.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArtifactTitle" type="xs:string">
				<xs:annotation>
					<xs:documentation>Name of the stored artifact.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArtifactAuthor" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of the author of the artifact. Could be a person or some other entity.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArtifactCreationDate" type="SeparatedDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date the artifact was created.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArtifactDescription" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Description or background details on the artifact and/or the reasons for its inclusion in the portfolio.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArtifactSource" type="SourceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Either an indication that the artifact was self-reported or the contact information for the entity that generated or supplied the artifact. Can be used for informal authentication of official artifacts.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArtifactClassification" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Element for portfolio system classification of artifacts. Would need to be supported by a mutually defined classification taxonomy.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArtifactMediaIdentifierCode" type="ArtifactMediaIdentifierCodeSimpleType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Media type represented by the stored artifact.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArtifactLocationURL" type="core:URLAddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>URL for Internet-based artifacts stored in the portfolio.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArtifactFileName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>File name for artifacts that are documents stored in the portfolio.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArtifactObjectLink" type="ObjectLinkType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Object link to another node or object related to this artifact. For example it could connect this artifact to a related event documented in the portfolio.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AuthorType">
		<xs:annotation>
			<xs:documentation>A complex type describing the author of a comment.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="xs:string">
				<xs:annotation>
					<xs:documentation>The title given to someone, usually at birth. The name of the author of the comment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrganizationName" type="core:OrganizationNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of Non-person entity such as Organization, Institution, Agency or Business.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Relationship" type="RelationshipSimpleType">
				<xs:annotation>
					<xs:documentation>The relationship of the comment's author to the portfolio owner.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EventClassificationType">
		<xs:annotation>
			<xs:documentation>A complex type describing the classification of an event.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EventPrimaryClassification" type="EventPrimaryClassificationCodeSimpleType">
				<xs:annotation>
					<xs:documentation>The primary classification of an event.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="ActivityCode" type="core:StudentActivityCodeType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>A subclassification for an event that is an activity.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AcademicProgramDegreeLevel" type="core:AcademicProgramDegreeLevelCodeType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The level of the degree for which the applicant is applying.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SeparatedDateType">
		<xs:annotation>
			<xs:documentation>A complex type that separates the date into separate day, month, and year elements</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Day" type="xs:gDay" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The day part of the specified date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Month" type="xs:gMonth" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The month part of the specified date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Year" type="xs:gYear" minOccurs="0">
